    "version": "0.2.0",
    "configurations": [
        
        {
            "name": "Python Debugger: main File without Arguments",
            "type": "debugpy",
            "request": "launch",
            "program": "my_python_template/my_python_template/main.py",
            "console": "integratedTerminal"
        },
        {
            "name": "Python Debugger: Current File with Arguments",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": [
                "${command:pickArgs}"
            ]
        },
        {
            "name": "Python Debugger: Current File without Arguments",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Python Debugger: main File with Arguments",
            "type": "debugpy",
            "request": "launch",
            "program": "my_python_template/my_python_template/main.py",
            "console": "integratedTerminal",
            "args": [
                "${command:pickArgs}"
            ]
        }
    ]
}